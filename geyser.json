{
  "file": "/data/plugins/Geyser-Spigot/config.yml",
  "ops": [
    {
      "$set": {
        "path": "$.bedrock.enable-proxy-protocol",
        "value": "${USES_PROXY_PROTOCOL}",
        "value-type": "bool"
      }
    },
    {
      "$set": {
        "path": "$.remote.use-proxy-protocol",
        "value": "${USES_PROXY_PROTOCOL}",
        "value-type": "bool"
      }
    }
  ]
}
